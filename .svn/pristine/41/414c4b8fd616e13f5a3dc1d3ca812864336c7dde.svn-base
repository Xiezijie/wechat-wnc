@import"/res/common.wxss";

.content{
  background-color: #fff;
  padding-top: 78rpx;
  text-align: center;
  padding-bottom: 64rpx;
}
.icon-box-title{
  font-size: 40rpx;
  font-weight: 700;
  color: #000;
  margin: 38rpx 0 24rpx;
}
.icon-box-desc{
  width: 640rpx;
  margin: auto;
  border-bottom: 1rpx solid #e1e1e1;
  padding-bottom: 40rpx;
}

/*** 按钮区域 ***/
.btnBody{
  margin-top: 74rpx;
}
.btnBody view{
  margin: 0 20rpx;
  border-radius: 5rpx;
  font-size: 36rpx;
  margin-bottom: 64rpx;  
}
.btnBody view:last-child{
  margin-bottom: 0;
}